{
    "name": "react-typescript",
    "version": "0.0.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/basarat/react-typescript.git"
    },
    "scripts": {
        "build": "webpack -p",
        "start": "webpack-dev-server -d --content-base ./public",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@types/react": "16.4.7",
        "@types/react-dom": "16.0.6",
        "lodash": "^4.17.10",
        "pluralize": "^7.0.0",
        "react": "16.4.2",
        "react-dom": "16.4.2",
        "react-redux": "^5.0.7",
        "redux": "^4.0.0",
        "redux-devtools-extension": "^2.13.5",
        "ts-loader": "4.4.2",
        "typescript": "3.0.1",
        "webpack-cli": "3.1.0",
        "webpack-dev-server": "3.1.5"
    },
    "devDependencies": {
        "@types/jest": "^23.3.1",
        "jest": "^23.5.0",
        "ts-jest": "^23.1.3",
        "webpack": "^4.16.4"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
